main
	check digits
	check duplicates
	check sorted
	check size
		if (size <= 5)
			sort small
		else
			sort large

sort large
	pushb 2x
	find max and min in b
	while stack_a has more than 3 elements
		for each element of stack_a find where it fits in stack_b and store number of rotations in ra and rb
		rotate stacks
	sort small stack_a
	push to stack_a
	order stack_a
